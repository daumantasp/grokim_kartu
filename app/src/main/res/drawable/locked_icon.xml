<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="?attr/lockedIconBackgroundColor" />
            <padding
                android:bottom="16dp"
                android:left="16dp"
                android:right="16dp"
                android:top="16dp" />
        </shape>
    </item>
    <item>
        <bitmap android:src="@drawable/ic_locked" android:tint="?attr/lockedIconColor" />
    </item>
</layer-list>