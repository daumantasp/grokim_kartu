    <attr name="secondaryTextInputHintColor" format="color" />
    <attr name="secondaryTextInputBoxStrokeColor" format="color" />

    <attr name="selectPhotoButtonBackgroundColor" format="color" />
    <attr name="selectPhotoButtonIconColor" format="color" />

    <attr name="bottomMenuBackgroundColor" format="color" />
