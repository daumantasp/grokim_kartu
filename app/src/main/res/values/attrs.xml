    <attr name="StatusBarMainColor" format="color" />

    <attr name="iconInHeaderColor" format="color" />

    <attr name="rowBackgroundColor" format="color" />

    <declare-styleable name="ButtonViewElement">
        <attr name="text" format="string" />
        <attr name="type" format="enum">
            <enum name="primaryInAuth" value="0" />
            <enum name="primary" value="1" />
            <enum name="secondary" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SpinnerViewElement">
        <attr name="unitDimensions" format="dimension" />
        <attr name="color" format="color" />
    </declare-styleable>

    <declare-styleable name="InitialsViewElement">
        <attr name="textSize" format="dimension" />
        <attr name="initialsType" format="enum">
            <enum name="rectangular" value="0" />
            <enum name="oval" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="HeaderViewElement">
        <attr name="title" format="string" />
        <attr name="showIcon" format="boolean" />
        <attr name="showBottomBorder" format="boolean" />
        <attr name="headerType" format="enum">
            <enum name="standart" value="0" />
            <enum name="withBack" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RowViewElement">
        <attr name="rowTitle" format="string" />
        <attr name="rowValue" format="string" />
        <attr name="rowShowArrow" format="boolean" />
    </declare-styleable>
</resources>